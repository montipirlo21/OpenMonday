{
  "schema": "Schemas/schema_2025-01.graphql",
  "documents": "**/*.graphql",
  "extensions": {
    "strawberryShake": {
      "namespace": "OpenMonday.Core.strawberryShake",
      "name": "MondayClient",
      "url": "https://api.monday.com/v2/",
      "dependencyInjection": true,
      "records": {
        "inputs": false,
        "entities": false
